{
  "name": "next-elysia-monorepo",
  "private": true,
  "packageManager": "bun@1.0.35",
  "workspaces": ["packages_monorepo/*"],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
